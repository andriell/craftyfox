<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.github.andriell.gui.product.ProductsWorkArea">
  <grid id="27dc6" binding="rootPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="fc0e1" binding="paginationPanel" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="dd2f7" class="javax.swing.JLabel">
            <constraints/>
            <properties>
              <text value="Page:"/>
            </properties>
          </component>
          <component id="7fc0b" class="javax.swing.JButton" binding="prevButton">
            <constraints/>
            <properties>
              <margin top="0" left="7" bottom="0" right="7"/>
              <preferredSize width="46" height="24"/>
              <text value="&lt;&lt;"/>
            </properties>
          </component>
          <component id="a3058" class="javax.swing.JTextField" binding="pageTextField">
            <constraints/>
            <properties>
              <columns value="3"/>
              <text value="0"/>
            </properties>
          </component>
          <component id="51bf4" class="javax.swing.JButton" binding="nextButton">
            <constraints/>
            <properties>
              <margin top="0" left="7" bottom="0" right="7"/>
              <preferredSize width="46" height="24"/>
              <text value="&gt;&gt;"/>
            </properties>
          </component>
          <component id="b7312" class="javax.swing.JLabel">
            <constraints/>
            <properties>
              <text value="In page:"/>
            </properties>
          </component>
          <component id="913c5" class="javax.swing.JTextField" binding="inPageTextField">
            <constraints/>
            <properties>
              <columns value="3"/>
              <text value="20"/>
            </properties>
          </component>
          <component id="813e3" class="javax.swing.JLabel" binding="totalLabel">
            <constraints/>
            <properties>
              <text value="Total: 0"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="9cc43" binding="filterPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="West"/>
        <properties/>
        <border type="none"/>
        <children/>
      </grid>
      <scrollpane id="1ee28">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="9ee3" binding="dataPanel" layout-manager="CardLayout" hgap="0" vgap="0">
            <constraints/>
            <properties/>
            <border type="none"/>
            <children/>
          </grid>
        </children>
      </scrollpane>
    </children>
  </grid>
</form>
